		<script type="text/javascript">
		function highlight(gids) {
			var gid = gids.split(",");
			for(i = 0; i < gid.length; i++){
				if ( $("#grid"+gid[i]).hasClass("gridTechnicallyFree") ) {
					$("#grid"+gid[i]).addClass("gridTechnicallyFreeHighlight");
				}
				$("#grid"+gid[i]).removeClass("gridFree");
				$("#grid"+gid[i]).removeClass("gridTechnicallyFree");
				$("#grid"+gid[i]).removeClass("gridBusy");
				$("#grid"+gid[i]).addClass("gridHighlight");
			}
		}
