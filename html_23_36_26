			$("#grid1533242700000").addClass("gridBusy");
			
			
			
		}
		function policeColumns() {
			$(".grid").css("width", "100%");
			$(".grid").css("max-width", "100%");
			var columns = $(".gridDay:visible, .paging");
			var before = $(".pagingBefore");
			var after = $(".pagingAfter");
			columns.addClass("wideCol");
			columns.removeClass("mediumCol");
			columns.removeClass("narrowCol");
			$("#gridPage").removeClass("singleCol");
			if (before.offset().top != after.offset().top) {
				columns.addClass("mediumCol");
			}
			if (before.offset().top != after.offset().top) {
				columns.addClass("narrowCol");
			}
			if (before.offset().top != after.offset().top) {
				columns.removeClass("narrowCol");
				columns.removeClass("mediumCol");
				columns.removeClass("wideCol");
				$("#gridPage").addClass("singleCol");
				$(".noFreeMessage").removeClass("modal");
			}
			else {
				var w = 1;
				columns.each(function(){
					w = w + $(this).outerWidth(true);
				});
				if ($(".grid").css("padding-left")) w = w + parseInt($(".grid").css("padding-left"));
				if ($(".grid").css("padding-right")) w = w + parseInt($(".grid").css("padding-right"));
				if ($(".grid").css("border-left-width")) w = w + parseInt($(".grid").css("border-left-width"));
				if ($(".grid").css("border-right-width")) w = w + parseInt($(".grid").css("border-right-width"));
				$(".grid").css("width", w + "px");
				$(".grid").css("max-width", w + "px");
				$(".grid").css("margin", "0px auto");
				$(".noFreeMessage").addClass("modal");
			}
		}
		function updateTimeZone() {
			$("#gridPage").css("visibility", "hidden");
			$("#spinner").show();
			if ($("#continent").val() == "UTC") {
				$("#btz").val("UTC");
			}
			else {
				$("#btz").val($("#zone" + $("#continent").val()).val());
			}
			$("#timeZoneForm").submit();
		}
		
		$(document).ready(function() {
			if ($.datepicker.regional['pt-BR']) {
				$.datepicker.setDefaults($.datepicker.regional['pt-BR']);
			}
			else if ($.datepicker.regional['pt']) {
				$.datepicker.setDefaults($.datepicker.regional['pt']);
			}
			else {
				$.datepicker.setDefaults($.datepicker.regional['en-GB']);
			}
			$.datepicker.setDefaults({
				dateFormat: "yy-mm-dd",
				showOn: "both",
				buttonText: "Ir para a data",
				showAnim: "",
				constrainInput: false ,
				gotoCurrent: true
			});
			$("#jumpDate").datepicker();
			$("button.ui-datepicker-trigger").wrapInner("<span>");
			
			$(".pageButton").click(function(){
				$("#gridPage").css("visibility", "hidden");
				$("#spinner").show();
			});
			$("select#continent, select[data-zone]").change(updateTimeZone);
			$(".gridHeaderDayName, .gridHeaderDate").each(function(){
				$(this).html($(this).data("content"));
			});
			$("#spinner").hide();
			$("#gridPage").css("visibility", "visible");
			policeColumns();
			$(window).resize(policeColumns);
		});
		</script>
		
	</body>
</html>
